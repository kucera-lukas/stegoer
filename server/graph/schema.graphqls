  DESC
}

enum ErrorCode {
  DB_ERROR
  GRAPHQL_ERROR
  AUTHORIZATION_ERROR
  NOT_FOUND_ERROR
  VALIDATION_ERROR
  BAD_REQUEST_ERROR
